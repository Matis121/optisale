<div id="<%= dom_id product %>" class="w-full sm:w-auto my-5 space-y-5">
  <div>
    <strong class="block font-medium mb-1">Name:</strong>
    <%= product.name %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Sku:</strong>
    <%= product.sku %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Ean:</strong>
    <%= product.ean %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Tax rate:</strong>
    <%= product.tax_rate %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Cena:</strong>
    <%= product.gross_price %>
  </div>
  <div>
    <strong class="block font-medium mb-1">quantity:</strong>
    <%= product.quantity %>
  </div>
</div>
